{
  "strategy_name": "Conservative Arbitrage Strategy",
  "description": "Low-risk cross-DEX arbitrage opportunities with stable profit margins",
  "version": "1.0.0",
  "enabled": true,
  "risk_level": "low",
  
  "trading_config": {
    "mode": "ARBITRAGE_MODE",
    "max_position_size_usd": 5000.0,
    "max_slippage_percent": 0.5,
    "gas_price_multiplier": 1.2,
    "execution_timeout_ms": 2000,
    "enable_mev_protection": true,
    "mev_protection_level": "MEDIUM"
  },
  
  "arbitrage_settings": {
    "min_profit_percent": 0.3,
    "min_profit_usd": 10.0,
    "max_price_impact_percent": 1.0,
    "require_immediate_execution": true,
    "simulate_before_execute": true,
    "revert_on_failure": true
  },
  
  "supported_tokens": [
    "USDC", "USDT", "DAI", "WETH", "WBTC", 
    "UNI", "LINK", "AAVE", "COMP", "MKR"
  ],
  
  "dex_pairs": {
    "ethereum": {
      "primary_dexes": ["uniswap_v3", "sushiswap"],
      "secondary_dexes": ["curve", "balancer"],
      "gas_limit": 400000,
      "priority_fee_gwei": 1.0
    },
    "polygon": {
      "primary_dexes": ["quickswap", "sushiswap"],
      "secondary_dexes": ["curve", "balancer"],
      "gas_limit": 300000,
      "priority_fee_gwei": 30.0
    },
    "arbitrum": {
      "primary_dexes": ["uniswap_v3", "sushiswap"],
      "secondary_dexes": ["curve", "balancer"],
      "gas_limit": 400000,
      "priority_fee_gwei": 0.1
    }
  },
  
  "timing": {
    "scan_interval_ms": 500,
    "decision_timeout_ms": 200,
    "execution_delay_ms": 100,
    "cooldown_between_trades_ms": 5000,
    "price_staleness_threshold_ms": 1000
  },
  
  "risk_management": {
    "max_concurrent_arbitrages": 3,
    "max_daily_trades": 100,
    "max_exposure_per_token_usd": 10000.0,
    "emergency_stop_loss_percent": 2.0,
    "correlation_check_enabled": true
  },
  
  "profit_optimization": {
    "compound_profits": true,
    "reinvest_percentage": 80.0,
    "min_balance_reserve_usd": 1000.0,
    "gas_cost_optimization": true,
    "batch_similar_trades": true
  },
  
  "monitoring": {
    "track_performance": true,
    "log_failed_arbitrages": true,
    "alert_on_unusual_patterns": true,
    "performance_benchmark": "market_neutral",
    "daily_report_enabled": true
  },
  
  "advanced": {
    "use_flashloans": false,
    "cross_chain_arbitrage": false,
    "mev_sandwich_protection": true,
    "dynamic_gas_pricing": true,
    "liquidity_aggregation": true,
    "statistical_arbitrage": false
  }
}
